{
	"extends": "../tsconfig.base.json",
	"include": ["src/**/*"],
	"exclude": ["test/**/*"],
	"compilerOptions": {
		"lib": ["ESNext"],
		"module": "CommonJS",
		"target": "ESNext",
		"outDir": "build",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"noImplicitAny": false,
		"noUnusedLocals": true,
		"resolveJsonModule": true,
		"sourceMap": true
	}
}

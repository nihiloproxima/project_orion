{
	"esModuleInterop": true,
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Node 20",
	"_version": "20.1.0",
	"include": ["src/**/*"],
	"exclude": ["test/**/*"],
	"compilerOptions": {
		"lib": ["ESNext"],
		"module": "CommonJS",
		"target": "ESNext",
		"outDir": "build",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"noImplicitAny": false,
		"noUnusedLocals": true,
		"resolveJsonModule": true,
		"sourceMap": true
	}
}
